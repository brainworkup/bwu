  title = {Physician Preferences to Communicate Neuropsychological Results: {{Comparison}} of Qualitative Descriptors and a Proposal to Reduce Communication Errors},
  author = {Schoenberg, Mike R and Osborn, Katie E and Mahone, E Mark and Feigon, Maia and Roth, Robert M and Pliskin, Neil H},
  date = {2017-11},
  journaltitle = {Arch. Clin. Neuropsychol.},
