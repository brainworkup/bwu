@article{llanosSelfTranscendenceCollectiveTranscendence2022,
  title = {From {{Self-Transcendence}} to {{Collective Transcendence}}: {{In Search}} of the {{Order}} of {{Hierarchies}} in {{Maslow}}'s {{Transcendence}}},
